class fibonacciClass {
    static int num1 = 0;
    static int num2 = 1;
    static int num3;
    int range = 0;
    
        private static void fibonacci(int range) {
            if(range > 0){
        num3 = num1 + num2;
        num1 = num2;
        num2 = num3;
        System.out.print(" "+num3);
        fibonacci(range - 1);
        }
    }
    
    public static void main(String[] args){
        System.out.print(num1+ " " +num2);
        fibonacci(8);
    }
}
        
